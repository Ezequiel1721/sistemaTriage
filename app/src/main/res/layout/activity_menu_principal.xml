<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuPrincipal"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/ivBtnSalir"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_icono_salir"
        android:onClick="SalirApp" />

    
    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/logo_principal_1"
        android:layout_marginTop="80dp"
        android:layout_gravity="center_horizontal"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal">

        <ImageView
            android:id="@+id/ivBtnIniciarSesion"
            android:layout_width="168dp"
            android:layout_height="46dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="112dp"
            android:background="@drawable/ic_icono_sesion"
            android:onClick="AInicioDeSesion" />

    </LinearLayout>


</LinearLayout>